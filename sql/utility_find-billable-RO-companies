SELECT
O.*, 
COUNT(DISTINCT(P.user_id)) AS UserCount, 
COUNT(DISTINCT(E.event_id)) AS EventCount, 
COUNT(DISTINCT(T.time_id)) AS TimeEntryCount
FROM `organizations` O
LEFT JOIN `events` E ON (O.organization_id = E.organization_id)
LEFT JOIN `permissions` P ON (O.organization_id = P.organization_id)
LEFT JOIN `users` U ON (U.user_id = P.user_id)
LEFT JOIN `times` T ON (E.event_id = T.event_id)
GROUP BY O.organization_id
HAVING EventCount = 0
AND TimeEntryCount > 0